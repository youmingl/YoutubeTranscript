{
    "manifest_version": 3,
    "name": "Youtube Transcript",
    "version": "1.0",
    "description": "Display all transcript for the video",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      },
      "action": {
        "default_title": "Open the popup",
        "default_popup": "scripts/index.html"
    },
    "background": {
      "service_worker": "scripts/background.js"
    },
      "content_scripts": [
        {
          "js": ["scripts/app.js"],
          "matches": [
            "*://youtube.com/*",
            "*://www.youtube.com/*",
            "*://m.youtube.com/*"
          ],
          "exclude_matches": ["*://*.music.youtube.com/*"]
        }
      ],
      "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'",
        "sandbox": "sandbox allow-scripts; script-src 'self' 'https://apis.google.com/' 'https://accounts.google.com/'  'https://www.googleapis.com' 'https://ajax.googleapis.com'; object-src 'self'"
        },
      "oauth2": {
        "client_id": "267861215901-l0abhe535lq2qa8neic1vqc23jirak1n.apps.googleusercontent.com",
        "scopes":["https://www.googleapis.com/auth/userinfo.email"]
      },
      "permissions":[
        "identity"
      ],
      "key": ""
  }