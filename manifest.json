{
    "manifest_version": 3,
    "name": "Youtube Transcript",
    "version": "1.0",
    "description": "Display all transcript for the video",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      },
      "action": {
        "default_popup": "scripts/index.html",
        "default_title": "Open the popup"
    },
      "content_scripts": [
        {
          "js": ["scripts/content2.js"],
          "matches": [
            "*://youtube.com/*",
            "*://www.youtube.com/*",
            "*://m.youtube.com/*"
          ],
          "exclude_matches": ["*://*.music.youtube.com/*"]
        }
      ],
      "oauth2": {
        "client_id": "267861215901-ers016luqsc9lp9evp0srrbc0pdpgukt.apps.googleusercontent.com",
        "scopes":[""]
      },
      "permissions":[
        "identity"
      ],
      "key": ""
  }