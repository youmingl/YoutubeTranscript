{
    "manifest_version": 3,
    "name": "Youtube Transcript",
    "version": "1.0",
    "description": "Display all transcript for the video",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      },
      "permissions": [
        "storage"
      ],
      "content_scripts": [
        {
          "js": ["scripts/content2.js"],
          "matches": [
            "*://youtube.com/*",
            "*://www.youtube.com/*",
            "*://m.youtube.com/*"
          ],
          "exclude_matches": ["*://*.music.youtube.com/*"]
        }
      ],
      "action": {}
  }